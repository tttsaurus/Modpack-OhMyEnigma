{
    "variables": {
        "percentage_text": "(floor(percentage * 100)) + '.' + (floor(percentage * 1000) % 10) + '%'",
        "loading_time_text": "floor(time / 60) + 'm ' + floor((time % 60) / 10) + floor(time % 60 % 10) + 's'",

        "pixel_size": 1.3,
        "progress_bar_pixel_width": 281.0,
        "progress_bar_pixel_height": 10.0,

        "anim_0_frame_duration": 0.7,
        "anim_0_pixel_height": 143.0,
        "anim_0_frame_count": 11,
        "anim_0_frame_pixel_height": "anim_0_pixel_height / anim_0_frame_count",
        "anim_0_uv_height": "1.0 / anim_0_frame_count",
        "anim_0_uv_y": "anim_0_frame_pixel_height * floor((time % anim_0_frame_duration) / anim_0_frame_duration * anim_0_frame_count) / anim_0_pixel_height"
    },
    "functions": {
        "ternary(int in0, int in1, float out0, float out1)": "out0 * (1 - min(1, abs(in0 - in1))) + out1 * min(1, abs(in0 - in1)) + 0.0",

        "background_0_pixel_width()": "1401.0",
        "background_0_pixel_height()": "891.0",
        "background_1_pixel_width()": "4000.0",
        "background_1_pixel_height()": "2250.0",
        "background_2_pixel_width()": "4000.0",
        "background_2_pixel_height()": "2250.0",
        "background_3_pixel_width()": "4000.0",
        "background_3_pixel_height()": "2250.0",
        "background_4_pixel_width()": "4000.0",
        "background_4_pixel_height()": "2250.0",

        "background_width_raw_4(int index)": "ternary(index, 4, background_2_pixel_width(), 0)",
        "background_width_raw_3(int index)": "ternary(index, 3, background_2_pixel_width(), index > 3 ? background_width_raw_4(index) : 0)",
        "background_width_raw_2(int index)": "ternary(index, 2, background_2_pixel_width(), index > 2 ? background_width_raw_3(index) : 0)",
        "background_width_raw_1(int index)": "ternary(index, 1, background_1_pixel_width(), index > 1 ? background_width_raw_2(index) : 0)",
        "background_width_raw_0(int index)": "ternary(index, 0, background_0_pixel_width(), index > 0 ? background_width_raw_1(index) : 0)",
        
        "background_height_raw_4(int index)": "ternary(index, 2, background_2_pixel_height(), 0)",
        "background_height_raw_3(int index)": "ternary(index, 2, background_2_pixel_height(), index > 3 ? background_height_raw_4(index) : 0)",
        "background_height_raw_2(int index)": "ternary(index, 2, background_2_pixel_height(), index > 2 ? background_height_raw_3(index) : 0)",
        "background_height_raw_1(int index)": "ternary(index, 1, background_1_pixel_height(), index > 1 ? background_height_raw_2(index) : 0)",
        "background_height_raw_0(int index)": "ternary(index, 0, background_0_pixel_height(), index > 0 ? background_height_raw_1(index) : 0)",
        
        "background_width(int index)": "screen_width",
        "background_height(int index)": "background_height_raw_0(index) / (background_width_raw_0(index) / screen_width)",

        "slideshow_count()": "2",
        "slideshow_each_duration()": "5.0",
        "slideshow_total_duration()": "slideshow_each_duration() * slideshow_count()",
        "slideshow_ordinal_index(float time)": "floor((time % slideshow_total_duration()) / slideshow_total_duration() * slideshow_count())",
        "slideshow_prev_index(float time)": "1",
        "slideshow_next_index(float time)": "1"
    },
    "renders": [
        {
            "image": {
                "parent": "builtin/image",
                "image": "customloadingscreen:in_use/textures/background_base.png"
            }
        },
        {
            "image": {
                "parent": "builtin/slideshow",
                "image": "customloadingscreen:in_use/textures/background/#.png",
                "frame": "slideshow_ordinal_index(time)",
                "position_type": "CENTER",
                "offset_pos": "CENTER",
                "colour": "0xFF_FF_FF_FF",
                "position": {
                    "x": "0",
                    "y": "0",
                    "width": "background_width(slideshow_ordinal_index(time))",
                    "height": "background_height(slideshow_ordinal_index(time))"
                },
                "texture": {
                    "x": "0",
                    "y": "0",
                    "width": "1",
                    "height": "1"
                }
            }
        },
        {
            "image": {
                "parent":"builtin/image",
